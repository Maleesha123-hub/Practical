public class Demo {
    public static void main(String[] args) {
        List<Integer> integers = Arrays.asList(5, 7, 3, 2, 4, 8, 6);

        for (int i = 0; i < integers.size(); i++) {
            System.out.println("normal for loop :" + integers.get(i));
        }

        for (Integer x : integers) {
            System.out.println("enhanced for loop :" + x);
        }

        integers.forEach(i -> System.out.println("for each loop :" + i));
    }
}
